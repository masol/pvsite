{
  "name": "pvsite",
  "version": "1.0.0",
  "description": "This project was bootstrapped with Fastify-CLI.",
  "main": "app.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "tap \"test/**/*.test.js\"",
    "start": "fastify start -l info -o app.js",
    "cmd": "NODE_OPTIONS=\"$NODE_OPTIONS --experimental-network-imports\" fastify start -T 0 -s cmd.sock -l info -P -o app.js -- --cmd",
    "dev": "fastify start -w --ignoreWatch=\"config pvdev src/frontend\" -l debug -P -o app.js"
  },
  "keywords": [],
  "private": true,
  "author": "lizhutang <lizhutang@spolo.org>",
  "license": "WWARE",
